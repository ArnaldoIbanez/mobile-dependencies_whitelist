{
    "users": ["engagement-it@mercadolibre.com"],
    "process_name": "growth-hermes",
    "cron": "00 08 * * *",
    "partition": {
      "name": "event_date",
      "type": "DATE",
      "format": "yyyy-MM-dd"
    },
    "extract": {
      "operation": "append",
      "site": "Sales",
      "columns": [{
        "name": "channel",
        "type": "UNICODE_STRING"
      }, {
        "name": "provider",
        "type": "UNICODE_STRING"
      }, {
        "name": "site_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "communication_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "communication_version",
        "type": "UNICODE_STRING"
      }, {
        "name": "segment_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "experiment_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "sent_date",
        "type": "DATE",
        "format": "yyyy-MM-dd"
      }, {
        "name": "sent",
        "type": "INTEGER"
      }, {
        "name": "dropped",
        "type": "INTEGER"
      }, {
        "name": "delivered",
        "type": "INTEGER"
      }, {
        "name": "deferred",
        "type": "INTEGER"
      }, {
        "name": "bounce",
        "type": "INTEGER"
      }, {
        "name": "open",
        "type": "INTEGER"
      }, {
        "name": "click",
        "type": "INTEGER"
      }, {
        "name": "spam_report",
        "type": "INTEGER"
      }, {
        "name": "unsubscribe",
        "type": "INTEGER"
      }, {
        "name": "group_unsubscribe",
        "type": "INTEGER"
      }, {
        "name": "group_resubscribe",
        "type": "INTEGER"
      }, {
        "name": "contacted",
        "type": "INTEGER"
      }, {
        "name": "unique_click",
        "type": "INTEGER"
      }, {
        "name": "unique_open",
        "type": "INTEGER"
      }],
      "sql": {
        "script": "growth/hermes.sql",
        "engine": "spark",
        "parameters": [{
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        }, {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }]
      }
    }
  }
