{
  "users": [
    "notifications-it@mercadolibre.com"
  ],
  "process_name": "notifications_push_metrics_governance",
  "cron": "00 03 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "columns": [{
      "name": "marketplace",
      "type": "UNICODE_STRING"
    },{
      "name": "site_id",
      "type": "UNICODE_STRING"
    }, {
      "name": "path",
      "type": "UNICODE_STRING"
    }, {
      "name": "device_status",
      "type": "UNICODE_STRING"
    }, {
      "name": "has_user",
      "type": "UNICODE_STRING"
    }, {
      "name": "app_version",
      "type": "UNICODE_STRING"
    }, {
      "name": "sent",
      "type": "INTEGER"
    }, {
      "name": "arrived",
      "type": "INTEGER"
    }, {
      "name": "no_reason_discarded",
      "type": "INTEGER"
    }, {
      "name": "track_without_arrived",
      "type": "INTEGER"
    }, {
      "name": "shown",
      "type": "INTEGER"
    }, {
      "name": "discarded",
      "type": "INTEGER"
    }, {
      "name": "open",
      "type": "INTEGER"
    }, {
      "name": "action_open",
      "type": "INTEGER"
    }, {
      "name": "dismiss",
      "type": "INTEGER"
    }, {
      "name": "auto_dismiss",
      "type": "INTEGER"
    }],
    "sql": {
      "script": "notifications/notifications-push-metrics-governance.sql",
      "engine": "spark",
      "parameters": [{
        "name": "send_date",
        "value": "DAY-3",
        "format": "yyyy-MM-dd"
      },{
        "name": "three_days_after_send_date",
        "value": "DAY",
        "format": "yyyy-MM-dd"
      }, {
        "name": "one_day_after_send_date",
        "value": "DAY-2",
        "format": "yyyy-MM-dd"
      }]
    }
  }
}
