        "type": "INTEGER"
      },
      {
        "name": "SI",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "cpg/attribution_cpg_2.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
