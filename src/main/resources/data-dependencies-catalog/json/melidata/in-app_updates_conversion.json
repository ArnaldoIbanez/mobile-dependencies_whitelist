{
    "users": [
      "mobile-arquitectura@mercadolibre.com"
    ],
    "process_name": "in-app_updates_conversion",
    "cron": "00 01 * * *",
    "partition": {
      "name": "ds",
      "type": "DATE"
    },
    "extract": {
      "operation": "append",
      "columns": [
        {
          "name": "business",
          "type": "UNICODE_STRING"
        },
        {
          "name": "site",
          "type": "UNICODE_STRING"
        },
        {
          "name": "Action",
          "type": "UNICODE_STRING"
        },
        {
          "name": "update_type",
          "type": "UNICODE_STRING"
        },
        {
          "name": "Flow_Type",
          "type": "UNICODE_STRING"
        },
        {
          "name": "How_Many",
          "type": "INTEGER"
        },
        {
          "name": "platform",
          "type": "UNICODE_STRING"
        }
      ],
      "sql": {
        "script": "mobile/in-app_updates/in-app_updates.sql",
        "engine": "spark",
        "parameters": [
          {
            "name": "param01",
            "value": "DAY-1",
            "format": "yyyy-MM-dd"
          },
          {
            "name": "param02",
            "value": "DAY-0",
            "format": "yyyy-MM-dd"
          }
        ]
      }
    }
  }