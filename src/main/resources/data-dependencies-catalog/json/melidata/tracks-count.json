{
  "users": [
    "martin.ciruzzi@mercadolibre.com"
  ],
  "process_name": "tracks_count",
  "cron": "45 10 * * *",
  "extract": {
    "operation": "destroy",
    "columns": [
      {
        "name": "ds",
        "type": "DATE",
        "format": "yyyy-MM-dd"
      },
      {
        "name": "business",
        "type": "UNICODE_STRING"
      },
      {
        "name": "site",
        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "total",
        "type": "INTEGER"
      }
    ],
    "sql": {

      "script": "melidata/tracks-count.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
