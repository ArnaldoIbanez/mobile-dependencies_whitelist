{
  "users": [
    "melidata_dev@mercadolibre.com"
  ],
  "process_name": "melidata_data_loss",
  "cron": "00 08 * * *",
  "partition": {
    "name": "track_date",
    "type": "DATE",
    "format": "yyyy-MM-dd"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "site",
        "type": "UNICODE_STRING"
      },
      {
        "name": "is_carrito",
        "type": "UNICODE_STRING"
      },
      {
        "name": "bt_orders_presence",
        "type": "DOUBLE"
      },
      {
        "name": "entities_presence",
        "type": "DOUBLE"
      },
      {
        "name": "checkouts_presence",
        "type": "DOUBLE"
      },
      {
        "name": "total_entities",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "melidata/melidata_data_loss.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-2",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
