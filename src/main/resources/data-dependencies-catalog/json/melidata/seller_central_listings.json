{
  "users": [
    "ezequiel.perez@mercadolibre.com"
  ],
  "process_name": "seller_central_listings",
  "cron": "00 07 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE",
    "format": "yyyy-MM-dd"
  },
  "extract": {
    "operation": "append",
    "site": "QA",
    "columns": [
      {
        "name": "id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "accion",
        "type": "UNICODE_STRING"
      },
      {
        "name": "tipo",
        "type": "UNICODE_STRING"
      },
      {
        "name": "plataforma",
        "type": "UNICODE_STRING"
      },
      {
        "name": "cantidad",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "seller_central/seller_central_listings.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}