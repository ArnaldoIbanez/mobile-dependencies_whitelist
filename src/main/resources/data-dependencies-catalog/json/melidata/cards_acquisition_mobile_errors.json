{
    "users": [
      "jonatan.urquiza@mercadolibre.com"
    ],
    "process_name": "data_cards_acquisition_errors",
    "cron": "00 03 * * *",
    "partition": {
      "name": "fecha",
      "type": "DATE"
    },
    "extract": {
      "operation": "append",
      "columns": [
        {
          "name": "platform",
          "type": "UNICODE_STRING"
        },
        {
          "name": "version",
          "type": "UNICODE_STRING"
        },
        {
          "name": "screen",
          "type": "UNICODE_STRING"
        },
        {
          "name": "unique_affected_users",
          "type": "INTEGER"
        },
        {
          "name": "total",
          "type": "INTEGER"
        }
      ],
      "sql": {
        "script": "mercadopago/cards_acquisition_mobile_errors.sql",
        "engine": "spark",
        "parameters": [
          {
            "name": "param01",
            "value": "DAY-1",
            "format": "yyyy-MM-dd"
          },
          {
            "name": "param02",
            "value": "DAY-0",
            "format": "yyyy-MM-dd"
          }
        ]
      }
    }
  }