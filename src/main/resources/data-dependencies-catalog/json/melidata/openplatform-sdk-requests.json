	"extract": {
		"operation": "append",
		"columns": [
			{
				"name": "request_date",
				"type": "DATE",
				"format": "yyyy-MM-dd"
			},
			{
				"name": "http_user_agent",
				"type": "UNICODE_STRING"
			},
			{
				"name": "total",
				"type": "INTEGER"
			}
		],
		"sql": {
			"script": "openplatform/sdk-requests.sql",
			"parameters": [
				{
					"name": "param01",
					"value": "DAY-1",
					"format": "yyyy-MM-dd"
				}
			]
		}
	}
}