{
  "users": [
    "desa@mercadoshops.com",
    "diego.zima@mercadolibre.com"
  ],
  "process_name": "mercadoshops-migration",
  "cron": "00 07 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "site": "MercadoShops",
    "columns": [
      {
        "name": "site_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "path",
        "type": "UNICODE_STRING"
      }, {
        "name": "uid",
        "type": "UNICODE_STRING"
      },{
        "name": "user_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "optin_card",
        "type": "UNICODE_STRING"
      }, {
        "name": "shop_domain",
        "type": "UNICODE_STRING"
      }
    ],
    "sql": {
      "script": "mercadoshops/migration.sql",
      "engine": "spark",
      "parameters": [{
        "name": "param01",
        "value": "DAY-1",
        "format": "yyyy-MM-dd"
      }, {
        "name": "param02",
        "value": "DAY-0",
        "format": "yyyy-MM-dd"
      }]
    }
  }
}
