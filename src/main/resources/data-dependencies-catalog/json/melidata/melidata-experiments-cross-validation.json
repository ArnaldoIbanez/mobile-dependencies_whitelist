{
  "users": [
    "patricio.benitez@mercadolibre.com"
  ],
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "process_name": "experiments_validation",
  "cron": "30 15 * * *",
  "extract": {
    "operation": "destroy",
    "columns": [
      {
        "name": "source",
        "type": "UNICODE_STRING"
      },
      {
        "name": "experiment_name",
        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "variant_id",
        "type": "INTEGER"
      },
      {
        "name": "sessions",
        "type": "INTEGER"
      },
      {
        "name": "congrats",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "melidata/melidata-experiments-cross-validation.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
