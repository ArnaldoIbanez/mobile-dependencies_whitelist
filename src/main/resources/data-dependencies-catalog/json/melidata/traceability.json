{
  "users": [
    "julio.cardenas@mercadolibre.cl",
     "bi-analytics@mercadolibre.com"
  ],
  "process_name": "data_traceability",
  "cron": "00 02 * * *",
  "partition": {
        "name": "fecha",
        "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "usuario",
        "type": "INTEGER"
      },
      {
        "name": "flow_init",
        "type": "INTEGER"
      },
      {
        "name": "flow_end",
        "type": "INTEGER"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "site",
        "type": "UNICODE_STRING"
      },
      {
        "name": "version",
        "type": "UNICODE_STRING"
      },
      {
        "name": "flow",
        "type": "UNICODE_STRING"
      },
      {
        "name": "flow_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "flow_from",
        "type": "UNICODE_STRING"
      }
    ],
    "sql": {
      "script": "mercadopago/traceability.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
