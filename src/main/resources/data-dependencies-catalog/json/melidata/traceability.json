{
  "users": [
    "lucio.carboneti@mercadolibre.com",
    "mpmobile_core@mercadolibre.com"
  ],
  "process_name": "data_traceability",
  "cron": "00 04 * * *",
  "partition": {
    "name": "fecha",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "flow_init",
        "type": "INTEGER"
      },
      {
        "name": "flow_end",
        "type": "INTEGER"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "site",
        "type": "UNICODE_STRING"
      },
      {
        "name": "business",
        "type": "UNICODE_STRING"
      },
      {
        "name": "flow",
        "type": "UNICODE_STRING"
      },
      {
        "name": "flow_from",
        "type": "UNICODE_STRING"
      }
    ],
    "sql": {
      "script": "mercadopago/traceability.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
