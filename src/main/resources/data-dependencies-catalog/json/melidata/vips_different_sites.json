{
  "users": [
    "melidata_dev@mercadolibre.com"
  ],
  "process_name": "vips_different_sites",
  "cron": "00 19 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE",
    "format": "yyyy-MM-dd"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "site_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "qty_users",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "melidata/vips_different_sites.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}

