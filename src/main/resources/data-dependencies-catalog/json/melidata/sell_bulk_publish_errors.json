      },
      {
      "name": "category",
      "type": "UNICODE_STRING"
      },
      {
        "name": "error_code",
        "type": "UNICODE_STRING"
      },
      {
        "name": "quantity",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "sell/sell-bulk-publish-errors.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}