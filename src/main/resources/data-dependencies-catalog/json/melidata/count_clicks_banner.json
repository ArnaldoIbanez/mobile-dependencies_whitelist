  "process_name": "count_clicks_banner",
  "cron": "00 07 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE",
    "format": "yyyy-MM-dd"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "Site",
        "type": "UNICODE_STRING"
      },
      {
        "name": "Plataforma",
        "type": "UNICODE_STRING"
      },
      {
        "name": "size",
        "type": "UNICODE_STRING"
      },
      {
        "name": "banner_name",
        "type": "UNICODE_STRING"
      },
        {
        "name": "SellerID",
        "type": "UNICODE_STRING"
      },
        {
        "name": "total",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "business-assurance/cantidaddeclicks.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
           "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
