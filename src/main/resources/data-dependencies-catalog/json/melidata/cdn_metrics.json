{
    "users": [
        "frontendcore@mercadolibre.com"
    ],
    "process_name": "cdn_metrics",
    "cron": "0 6 * * *",
    "partition": {
        "name": "ds",
        "type": "DATE"
    },
    "extract": {
        "operation": "append",
        "site": "Desarrollo-IT",
        "columns": [
            {
                "name": "site_id",
                "type": "UNICODE_STRING"
            },
            {
                "name": "device_platform",
                "type": "UNICODE_STRING"
            },
            {
                "name": "cdn_provider",
                "type": "UNICODE_STRING"
            },
            {
                "name": "connection_type",
                "type": "UNICODE_STRING"
            },
            {
                "name": "file_name",
                "type": "UNICODE_STRING"
            },
            {
                "name": "file_type",
                "type": "UNICODE_STRING"
            },
            {
                "name": "cache_status",
                "type": "UNICODE_STRING"
            },
            {
                "name": "file_size",
                "type": "DOUBLE"
            },
            {
                "name": "duration",
                "type": "DOUBLE"
            },
            {
                "name": "remote_ip",
                "type": "UNICODE_STRING"
            }
        ],
        "sql": {
            "script": "melidata/cdn_metrics.sql",
            "engine": "spark",
            "parameters": [
                {
                    "name": "dateFROM",
                    "value": "DAY-1",
                    "format": "yyyy-MM-dd"
                },
                {
                    "name": "dateTO",
                    "value": "DAY-0",
                    "format": "yyyy-MM-dd"
                }
            ]
        }
    }
}