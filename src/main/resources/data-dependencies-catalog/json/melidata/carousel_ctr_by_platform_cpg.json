      "site": "Sales",
      "columns": [
        {
          "name": "platform",
          "type": "UNICODE_STRING"
        },
        {
          "name": "prints",
          "type": "INTEGER"
        },
        {
          "name": "clicks",
          "type": "INTEGER"
        }
      ],
      "sql": {
        "script": "cpg/carousel_ctr_by_platform_cpg.sql",
        "engine": "spark",
        "parameters": [
          {
            "name": "param01",
            "value": "DAY-1",
            "format": "yyyy-MM-dd"
          },
          {
            "name": "param02",
            "value": "DAY-0",
            "format": "yyyy-MM-dd"
          }
        ]
      }
    }
  }