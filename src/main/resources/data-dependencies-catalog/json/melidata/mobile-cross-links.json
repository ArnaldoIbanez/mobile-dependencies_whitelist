{
    "users": [
      "mobile-arquitectura@mercadolibre.com"
    ],
    "process_name": "cross-links_conversion",
    "cron": "00 01 * * *",
    "partition": {
      "name": "ds",
      "type": "DATE"
    },
    "extract": {
      "operation": "append",
      "site": "CrossUnits",
      "columns": [
        {
          "name": "APP",
          "type": "UNICODE_STRING"
        },
        {
          "name": "SITE",
          "type": "UNICODE_STRING"
        },
        {
          "name": "APP_VERSION",
          "type": "UNICODE_STRING"
        },
        {
          "name": "PLATFORM",
          "type": "UNICODE_STRING"
        },
        {
          "name": "ACTION",
          "type": "UNICODE_STRING"
        },
        {
          "name": "REASON",
          "type": "UNICODE_STRING"
        },
        {
          "name": "RESULT",
          "type": "UNICODE_STRING"
        },
        {
          "name": "FRONTEND",
          "type": "UNICODE_STRING"
        },
        {
          "name": "HOW_MANY",
          "type": "INTEGER"
        }
      ],
      "sql": {
        "script": "mobile/ucrania/cross-links.sql",
        "engine": "spark",
        "parameters": [
          {
            "name": "param01",
            "value": "DAY-1",
            "format": "yyyy-MM-dd"
          },
          {
            "name": "param02",
            "value": "DAY-0",
            "format": "yyyy-MM-dd"
          }
        ]
      }
    }
  }