{
  "users": [
     "fventre@mercadolibre.com",
     "mpmobile_core@mercadolibre.com"
  ],
  "process_name": "data_apprater",
  "cron": "00 03 * * *",
  "partition": {
        "name": "fecha",
        "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
         "name": "type_track",
         "type": "UNICODE_STRING"
       },
       {
         "name": "count_track",
         "type": "INTEGER"
       },
       {
        "name": "site",
        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "version",
        "type": "UNICODE_STRING"
      }
    ],
    "sql": {
      "script": "mercadopago/apprater.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}