{
  "users": [
    "notifications-it@mercadolibre.com"
  ],
  "process_name": "notifications_push_metrics_v4",
  "cron": "00 03 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "destroy",
    "columns": [{
      "name": "marketplace",
      "type": "UNICODE_STRING"
    },{
      "name": "site_id",
      "type": "UNICODE_STRING"
    }, {
      "name": "path",
      "type": "UNICODE_STRING"
    }, {
      "name": "sent",
      "type": "INTEGER"
    }, {
      "name": "arrived",
      "type": "INTEGER"
    }, {
      "name": "discarded",
      "type": "INTEGER"
    }, {
      "name": "resent",
      "type": "INTEGER"
    }, {
      "name": "track_without_arrived",
      "type": "INTEGER"
    }],
    "sql": {
      "script": "notifications/notifications-push-count_v4.sql",
      "parameters": [{
        "name": "send_date",
        "value": "DAY-3",
        "format": "yyyy-MM-dd"
      },{
        "name": "three_days_after_send_date",
        "value": "DAY",
        "format": "yyyy-MM-dd"
      }, {
        "name": "one_day_after_send_date",
        "value": "DAY-2",
        "format": "yyyy-MM-dd"
      }]
    }
  }
}
