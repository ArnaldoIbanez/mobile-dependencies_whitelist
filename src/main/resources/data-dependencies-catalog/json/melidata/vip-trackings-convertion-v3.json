{
  "users": [
    "manuel.kenar@mercadolibre.com",
    "vip_it@mercadolibre.com"
  ],
  "process_name": "vip_trackings_convertion_v3",
  "cron": "00 11 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "site_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "total_vip",
        "type": "INTEGER"
      },
      {
        "name": "total_buy_action",
        "type": "INTEGER"
      },
      {
        "name": "total_buy_grats",
        "type": "INTEGER"
      },
      {
        "name": "total_vip_cart",
        "type": "INTEGER"
      },
      {
        "name": "total_cart_action",
        "type": "INTEGER"
      },
      {
        "name": "total_cart_grats",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "vip/vip-trackings-convertion-v3.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
