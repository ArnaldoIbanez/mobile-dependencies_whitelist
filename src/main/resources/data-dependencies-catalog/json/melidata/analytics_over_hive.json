{
  "users": [
    "melidata_dev@mercadolibre.com"
  ],
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "process_name": "analytics_over_hive",
  "cron": "00 04 * * *",
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "username",
        "type": "UNICODE_STRING"
      },
      {
        "name": "operation",
        "type": "UNICODE_STRING"
      },
      {
        "name": "type",
        "type": "UNICODE_STRING"
      },
      {
        "name": "table_read",
        "type": "UNICODE_STRING"
      },
      {
        "name": "error_message",
        "type": "UNICODE_STRING"
      },
      {
        "name": "query_duration",
        "type": "INTEGER"
      },
      {
        "name": "partitions_read",
        "type": "INTEGER"
      },
      {
        "name": "cpu_time",
        "type": "INTEGER"
      },
      {
        "name": "owner",
        "type": "UNICODE_STRING"
      },
      {
        "name": "application_id",
        "type": "UNICODE_STRING"
      }
    ],
    "sql": {
      "script": "melidata/analytics_over_hive.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
