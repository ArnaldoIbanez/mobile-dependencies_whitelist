{
  "users": ["catalog-import@mercadolibre.com"],
  "process_name": "sd-catuniv-metrics",
  "cron": "0 6 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "site": "Catalogo",
    "columns": [
      {
        "name": "id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "path",
        "type": "UNICODE_STRING"
      },
      {
        "name": "server_poolname",
        "type": "UNICODE_STRING"
      },
      {
        "name": "user_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "query_filter",
        "type": "UNICODE_STRING"
      },
      {
        "name": "domain_filter",
        "type": "UNICODE_STRING"
      },
      {
        "name": "site_filter",
        "type": "UNICODE_STRING"
      },
      {
        "name": "is_gtin",
        "type": "UNICODE_STRING"
      },
      {
        "name": "has_results",
        "type": "UNICODE_STRING"
      },
      {
        "name": "displayed_sources",
        "type": "UNICODE_STRING"
      },
      {
        "name": "unknown_info_sources",
        "type": "UNICODE_STRING"
      },
      {
        "name": "product_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "product_source",
        "type": "UNICODE_STRING"
      },
      {
        "name": "product_external_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "external_domain",
        "type": "UNICODE_STRING"
      },
      {
        "name": "predicted_domain",
        "type": "UNICODE_STRING"
      },
      {
        "name": "selected_domain",
        "type": "UNICODE_STRING"
      },
      {
        "name": "external_attribute_key",
        "type": "UNICODE_STRING"
      },
      {
        "name": "external_attribute_value",
        "type": "UNICODE_STRING"
      }
    ],
    "sql": {
      "script": "structure_data/catalog_import-search-metrics.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
