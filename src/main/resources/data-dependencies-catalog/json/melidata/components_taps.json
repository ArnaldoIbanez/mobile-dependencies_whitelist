{
  "users": [
    "julio.cardenas@mercadolibre.cl"
  ],
  "process_name": "components_taps",
  "cron": "00 04 * * *",
  "partition": {
        "name": "ds",
        "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "site_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "version",
        "type": "UNICODE_STRING"
      },
      {
        "name": "path",
        "type": "UNICODE_STRING"
      },
      {
        "name": "component",
        "type": "UNICODE_STRING"
      },
      {
        "name": "taps",
        "type": "INTEGER"
      },
      {
        "name": "total",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "mercadopago/components_taps.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
