{
  "users": [
    "desa@mercadoshops.com",
    "diego.zima@mercadolibre.com"
  ],
  "process_name": "mercadoshops-usage",
  "cron": "00 07 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "site": "MercadoShops",
    "columns": [
      {
        "name": "type",
        "type": "UNICODE_STRING"
      }, {
        "name": "path",
        "type": "UNICODE_STRING"
      }, {
        "name": "cust_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "shop_name",
        "type": "UNICODE_STRING"
      }, {
        "name": "shop_domain",
        "type": "UNICODE_STRING"
      }, {
        "name": "opened_sidebar",
        "type": "UNICODE_STRING"
      }, {
        "name": "http_referer",
        "type": "UNICODE_STRING"
      }, {
        "name": "http_url",
        "type": "UNICODE_STRING"
      }, {
        "name": "ref_source",
        "type": "UNICODE_STRING"
      }, {
        "name": "utm_source",
        "type": "UNICODE_STRING"
      }, {
        "name": "utm_medium",
        "type": "UNICODE_STRING"
      }, {
        "name": "utm_campaign",
        "type": "UNICODE_STRING"
      }, {
        "name": "site_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "interactions",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "mercadoshops/usage.sql",
      "engine": "spark",
      "parameters": [{
        "name": "param01",
        "value": "DAY-1",
        "format": "yyyy-MM-dd"
      }, {
        "name": "param02",
        "value": "DAY-0",
        "format": "yyyy-MM-dd"
      }]
    }
  }
}
