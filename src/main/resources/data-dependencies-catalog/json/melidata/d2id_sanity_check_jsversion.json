{
  "users": [
    "melidata_dev@mercadolibre.com"
  ],
  "process_name": "d2id_sanity_check_jsversion",
  "cron": "00 15 * * *",
  "partition": {
    "name": "day",
    "type": "DATE",
    "format": "yyyy-MM-dd"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "js_version",
        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "site_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "business",
        "type": "UNICODE_STRING"
      },
      {
        "name": "qty_distinct_d2id",
        "type": "INTEGER"
      },
      {
        "name": "qty_distinct_d2id_with_n",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "melidata/d2id_sanity_check_jsversion.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
