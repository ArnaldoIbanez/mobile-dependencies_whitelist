{
  "users": [
    "notifications-it@mercadolibre.com"
  ],
  "process_name": "notifications_center",
  "cron": "00 06 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "destroy",
    "columns": [{
      "name": "total",
      "type": "INTEGER",
      "format": "yyyy-MM-dd"
    }, {
      "name": "notification_type",
      "type": "UNICODE_STRING"
    }, {
      "name": "event_type",
      "type": "UNICODE_STRING"
    }, {
      "name": "platform",
      "type": "UNICODE_STRING"
    }, {
      "name": "os_version",
      "type": "UNICODE_STRING"
    }, {
      "name": "site_id",
      "type": "UNICODE_STRING"
    }, {
      "name": "marketplace",
      "type": "UNICODE_STRING"
    }, {
      "name": "app_version",
      "type": "UNICODE_STRING"
    }, {
      "name": "action_type",
      "type": "UNICODE_STRING"
    }],
    "sql": {
      "script": "notifications/notifications-center-count.sql",
      "engine": "spark",
      "parameters": [{
        "name": "param01",
        "value": "DAY-1",
        "format": "yyyy-MM-dd"
      }, {
        "name": "param02",
        "value": "DAY-0",
        "format": "yyyy-MM-dd"
      }]
    }
  }
}