{
  "users": [
    "melidata_dev@mercadolibre.com"
  ],
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "process_name": "unrepeated_tracks",
  "cron": "00 09 * * *",
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "version",
        "type": "UNICODE_STRING"
      },
      {
        "name": "unrepeated",
        "type": "INTEGER"
      },
      {
        "name": "total",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "melidata/unrepeated-tracks.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
