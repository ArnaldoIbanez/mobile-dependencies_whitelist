        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "reference",
        "type": "UNICODE_STRING"
      },
      {
        "name": "total",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "business-assurance/credits_consumer_journey_checkout.sql",
      "engine": "spark",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
