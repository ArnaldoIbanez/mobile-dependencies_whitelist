
{
    "users": ["juan.veltri@mercadolibre.com",
    "mauricio.minestrelli@mercadolibre.com",
    "dx@mercadopago.com"],
    "process_name": "feedback-devsite",
    "cron": "00 03 * * *",
    "partition": {
      "name": "date_created",
      "type": "UNICODE_STRING"
    },
    "extract": {
      "operation": "append",
      "site": "MercadoPago",
      "columns": [{
        "name": "ocurrencias",
        "type": "INTEGER"
      },{
        "name": "platform",
        "type": "UNICODE_STRING"
      }, {
        "name": "feedback_value",
        "type": "UNICODE_STRING"
      }, {
        "name": "url",
        "type": "UNICODE_STRING"
      }, {
        "name": "site",
        "type": "UNICODE_STRING"
      }, {
        "name": "lang",
        "type": "UNICODE_STRING"
      }],
      "sql": {
        "script": "dx/feedback-devsite.sql",
        "engine": "spark",
        "parameters": [{
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        }, {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }]
      }
    }
  }
