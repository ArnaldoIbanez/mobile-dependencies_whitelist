{
  "users": [
    "natalia.soria@mercadolibre.com"
  ],
  "process_name": "cart-events",
  "cron": "00 06 * * *",
  "partition": {
  "name": "sent_date",
  "type": "DATE"
   },
  "extract": {
    "operation": "append",
    "site": "Desarrollo-IT",
    "columns": [
      {
        "name": "site",
        "type": "UNICODE_STRING"
      },
      {
        "name": "device",
        "type": "UNICODE_STRING"
      },
      {
        "name": "path",
        "type": "UNICODE_STRING"
      },
      {
        "name": "cant_event",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "melidata/cart-events-a2c.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
} 
