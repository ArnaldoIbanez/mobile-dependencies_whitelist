{
  "users": [
    "mathias.miranda@mercadolibre.com"
  ],
  "process_name": "wallet_traffic",
  "cron": "00 07 * * *",
  "partition": {
        "name": "fecha",
        "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "site": "MercadoPago",
    "columns": [
      {
        "name": "site",
        "type": "UNICODE_STRING"
      },
      {
        "name": "bu",
        "type": "UNICODE_STRING"
      },
      {
        "name": "app_version",
        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "flow",
        "type": "UNICODE_STRING"
      },
       {
        "name": "Init_UID",
        "type": "INTEGER"
      },
      {
       "name": "Congrats_UID_Success",
        "type": "INTEGER"
      },
      {
        "name": "Congrats_UID_Rejected",
        "type": "INTEGER"
      },
      {
        "name": "Init_Session",
        "type": "INTEGER"
      },
      {
        "name": "Congrats_Session_Success",
        "type": "INTEGER"
      },    
      {
        "name": "Congrats_Session_Rejected",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "mercadopago/wallet_traffic.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
