{
    "users": ["engagement-it@mercadolibre.com"],
    "process_name": "growth-hermes-v2",
    "cron": "00 11 * * *",
    "partition": {
      "name": "event_date",
      "type": "DATE",
      "format": "yyyy-MM-dd"
    },
    "extract": {
      "operation": "append",
      "site": "Sales",
      "columns": [{
        "name": "site_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "path",
        "type": "UNICODE_STRING"
      }, {
        "name": "platform",
        "type": "UNICODE_STRING"
      }, {
        "name": "campaign_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "batch_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "communication_id",
        "type": "UNICODE_STRING"
      }, {
        "name": "sent_date",
        "type": "DATE",
        "format": "yyyy-MM-dd"
      }, {
        "name": "sent",
        "type": "INTEGER"
      }, {
        "name": "shown",
        "type": "INTEGER"
      }, {
        "name": "buyers_sent",
        "type": "INTEGER"
      }, {
        "name": "buyers_shown",
        "type": "INTEGER"
      }, {
        "name": "uninstalls",
        "type": "INTEGER"
      }, {
        "name": "apagado_notifs",
        "type": "INTEGER"
      }],
      "sql": {
        "script": "growth/hermes-v2.sql",
        "engine": "spark",
        "parameters": [{
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        }, {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }]
      }
    }
  }
