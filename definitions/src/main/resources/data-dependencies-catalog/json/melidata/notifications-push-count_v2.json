{
  "users": [
    "notifications-it@mercadolibre.com"
  ],
  "process_name": "notifications_push_v2",
  "cron": "00 06 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "columns": [{
      "name": "total",
      "type": "INTEGER"
    }, {
      "name": "event_type",
      "type": "UNICODE_STRING"
    }, {
      "name": "marketplace",
      "type": "UNICODE_STRING"
    }],
    "sql": {
      "script": "notifications/notifications-push-count_v2.sql",
      "parameters": [{
        "name": "send_date",
        "value": "DAY-4",
        "format": "yyyy-MM-dd"
      },{
        "name": "three_days_after_send_date",
        "value": "DAY-1",
        "format": "yyyy-MM-dd"
      }, {
        "name": "one_day_after_send_date",
        "value": "DAY-3",
        "format": "yyyy-MM-dd"
      }]
    }
  }
}