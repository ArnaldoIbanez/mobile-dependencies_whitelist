{
  "users": [
    "agustin.blanco@mercadolibre.com",
    "rodrigo.loguzzo@mercadolibre.com",
    "hernan.herrera@mercadolibre.com",
    "vip_it@mercadolibre.com"
  ],
  "process_name": "vip_trackings_convertion_v2",
  "cron": "00 02 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "site",
        "type": "UNICODE_STRING"
      },
      {
        "name": "device",
        "type": "UNICODE_STRING"
      },
      {
        "name": "total_vips",
        "type": "INTEGER"
      },
      {
        "name": "amount_buy",
        "type": "INTEGER"
      },
      {
        "name": "amount_cart",
        "type": "INTEGER"
      },
      {
        "name": "amount_buy_real",
        "type": "INTEGER"
      },
      {
        "name": "amount_cart_real",
        "type": "INTEGER"
      }

    ],
    "sql": {
      "script": "vip/vip-trackings-convertion-v2.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
