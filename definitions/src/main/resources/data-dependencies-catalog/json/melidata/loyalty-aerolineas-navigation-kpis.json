      },
      {
        "name": "clicks",
        "type": "INTEGER"
      },
      {
        "name": "click_rate",
        "type": "DOUBLE"
      }
    ],
    "sql": {
      "script": "loyalty/aerolineas-navigation-metrics.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
