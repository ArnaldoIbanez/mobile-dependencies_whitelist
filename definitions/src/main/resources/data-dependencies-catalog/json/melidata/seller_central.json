{
  "users": [
    "ezequiel.perez@mercadolibre.com",
    "sebastian.schmukliar@mercadolibre.com"
  ],
  "process_name": "seller_central_actions",
  "cron": "00 08 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "site": "QA",
    "columns": [
      {
        "name": "quantity",
        "type": "INTEGER"
      },
      {
        "name": "source",
        "type": "UNICODE_STRING"
      },
      {
        "name": "accion",
        "type": "UNICODE_STRING"
      },
      {
        "name": "columnas",
        "type": "UNICODE_STRING"
      }
    ],
    "sql": {
      "script": "seller_central/seller_central.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}