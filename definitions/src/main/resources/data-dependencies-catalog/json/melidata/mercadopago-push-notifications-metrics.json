{
  "users": [
    "german.bringas@mercadolibre.com"
  ],
  "process_name": "mercadopago-push-notifications-metrics",
  "cron": "10 00 * * *",
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "ds",
        "type": "DATE",
        "format": "yyyy-MM-dd"
      },
      {
        "name": "campaign_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "event_type",
        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "users",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "mercadopago/push-notifications-metrics.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}