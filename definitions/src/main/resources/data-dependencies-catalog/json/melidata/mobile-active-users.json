        "type": "UNICODE_STRING"
      },
      {
        "name": "siteid",
        "type": "UNICODE_STRING"
      },
      {
        "name": "userid",
        "type": "INTEGER"
      },
      {
        "name": "activitydate",
        "type": "DATE",
        "format": "yyyy-MM-dd"
      }
    ],
    "sql": {

      "script": "melidata/mobile-active-users.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY",
          "format": "yyyy-MM-dd"
        }
      ]
    }
