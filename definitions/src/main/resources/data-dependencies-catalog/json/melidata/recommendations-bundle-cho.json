{
  "users":
  [
    "recommendations_fe@mercadolibre.com, carlos.rodrigo@mercadolibre.com"
  ],
  "process_name": "recommendations_bundle_cho",
  "cron": "00 05 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract":
  {
    "operation": "append",
    "site": "MeliLake",
    "columns":
    [
      {
        "name": "site_id",
        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "buy_intentions_from_combo",
        "type": "INTEGER"
      },
      {
        "name": "total_buy_intentions",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "recommendations/bundle-cho.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}
