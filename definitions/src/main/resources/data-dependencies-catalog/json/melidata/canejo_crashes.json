{
  "users": [
    "maria.mena@mercadolibre.cl"
  ],
  "process_name": "canejo-crashes",
  "cron": "00 01 * * *",
  "partition": {
    "name": "date_only",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "columns": [
      {
        "name": "hour_only",
        "type": "UNICODE_STRING"
      },
      {
        "name": "platform",
        "type": "UNICODE_STRING"
      },
      {
        "name": "dpto",
        "type": "UNICODE_STRING"
      },
      {
        "name": "app_version",
        "type": "UNICODE_STRING"
      },
      {
        "name": "business",
        "type": "UNICODE_STRING"
      },
      {
        "name": "uu_with_crashes_per_hour",
        "type": "INTEGER"
      },
      {
        "name": "crashes_per_hour",
        "type": "INTEGER"
      },
      {
        "name": "uu_with_warnings_per_hour",
        "type": "INTEGER"
      },
      {
        "name": "warnings_per_hour",
        "type": "INTEGER"
      },
      {
        "name": "uu_per_hour",
        "type": "INTEGER"
      },
      {
        "name": "screenviews_per_hour",
        "type": "INTEGER"
      }
    ],
    "sql": {
      "script": "canejo/crashes.sql",
      "parameters": [
        {
          "name": "param01",
          "value": "DAY-1",
          "format": "yyyy-MM-dd"
        },
        {
          "name": "param02",
          "value": "DAY-0",
          "format": "yyyy-MM-dd"
        }
      ]
    }
  }
}