{
  "users": [
    "notifications-it@mercadolibre.com"
  ],
  "process_name": "notifications_push_metrics_v3",
  "cron": "00 06 * * *",
  "partition": {
    "name": "ds",
    "type": "DATE"
  },
  "extract": {
    "operation": "append",
    "columns": [{
      "name": "marketplace",
      "type": "UNICODE_STRING"
    }, {
      "name": "event",
      "type": "UNICODE_STRING"
    }, {
      "name": "site_id",
      "type": "UNICODE_STRING"
    }, {
      "name": "path",
      "type": "UNICODE_STRING"
    }, {
      "name": "total",
      "type": "INTEGER"
    }],
    "sql": {
      "script": "notifications/notifications-push-count_v3.sql",
      "parameters": [{
        "name": "send_date",
        "value": "DAY-4",
        "format": "yyyy-MM-dd"
      },{
        "name": "three_days_after_send_date",
        "value": "DAY-1",
        "format": "yyyy-MM-dd"
      }, {
        "name": "one_day_after_send_date",
        "value": "DAY-3",
        "format": "yyyy-MM-dd"
      }]
    }
  }
}