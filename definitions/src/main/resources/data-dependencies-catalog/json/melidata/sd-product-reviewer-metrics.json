{
    "users": [
        "catalog_admin_devs@mercadolibre.com"
    ],
    "process_name": "sd-product-reviewer-metrics",
    "cron": "0 6 * * *",
    "partition": {
        "name": "ds",
        "type": "DATE"
    },
    "extract": {
        "operation": "append",
        "site": "Catalogo",
        "columns": [
            {
                "name": "review_id",
                "type": "UNICODE_STRING"
            },
            {
                "name": "review_version",
                "type": "INTEGER"
            },
            {
                "name": "review_status",
                "type": "UNICODE_STRING"
            },
            {
                "name": "domain_id",
                "type": "UNICODE_STRING"
            },
            {
                "name": "publication_id",
                "type": "UNICODE_STRING"
            },
            {
                "name": "sheet_id",
                "type": "UNICODE_STRING"
            },
            {
                "name": "time",
                "type": "INTEGER"
            },
            {
                "name": "user_initials",
                "type": "UNICODE_STRING"
            },
            {
                "name": "action_tag",
                "type": "UNICODE_STRING"
            },
            {
                "name": "error_desc",
                "type": "UNICODE_STRING"
            },
            {
                "name": "error_value",
                "type": "INTEGER"
            },
            {
                "name": "comments_desc",
                "type": "UNICODE_STRING"
            },
            {
                "name": "comments_value",
                "type": "INTEGER"
            }
        ],
        "sql": {
            "script": "structure_data/product_reviewer.sql",
            "parameters": [
                {
                    "name": "param01",
                    "value": "DAY-1",
                    "format": "yyyy-MM-dd"
                },
                {
                    "name": "param02",
                    "value": "DAY-0",
                    "format": "yyyy-MM-dd"
                }
            ]
        }
    }
}