{
	"users": [
		"wesley.nascimento@mercadolivre.com"
	],
	"process_name": "openplatform_request_api_daily",
	"cron": "00 05 * * *",
	"extract": {
		"operation": "overwrite",
		"columns": [
			{
				"name": "request_date",
				"type": "DATE",
				"format": "yyyy-MM-dd"
			},
			{
				"name": "total_requests",
				"type": "INTEGER"
			}
		],
		"sql": {
			"script": "openplatform/requests_api_daily.sql",
			"parameters": [
				{
					"name": "param01",
					"value": "DAY-7",
					"format": "yyyy-MM-dd"
				},
				{
					"name": "param02",
					"value": "DAY-0",
					"format": "yyyy-MM-dd"
				}
			]
		}
	}
}